-left: 7.5%;\r\n    margin-top: -2.5%;\r\n    margin-bottom: 2.5%;\r\n\r\n    width: 80%;\r\n}\r\n\r\n.intro{\r\n    color: #E9E8E8;\r\n    font-size: xx-large;\r\n    width: 75%;\r\n    padding-left: 15rem;\r\n}\r\n\r\n#world{\r\n    position: fixed;\r\n}\r\n\r\n\r\n.info{\r\n    position: absolute;\r\n    top: 230%;\r\n    width: 100vw;\r\n    height: 100vh;\r\n    /* background-color: #913175; */\r\n\r\n}\r\n\r\n.personal_info_section_heading{\r\n    all: unset;\r\n    display: flex;\r\n    font-size: 6rem; \r\n    padding: 6.5rem 15rem;\r\n    font-weight: 500;\r\n    color: #E9E8E8;\r\n}\r\n\r\n.input{\r\n    display: flex;\r\n    justify-content: space-evenly;\r\n    margin-right: 5%;\r\n}\r\n\r\n\r\n.input_area{\r\n    /* position: absolute; */\r\n    background-color: #E9E8E8;\r\n    width: 30vw;\r\n    height: auto ;\r\n}\r\n\r\n.input_date{\r\n    display: flex;\r\n}\r\n\r\n.verticle_line{\r\n    border-left:#E9E8E8 solid 3px;\r\n    height: auto;\r\n}\r\n\r\n\r\n.input_location{\r\n    /* figure out what this does */\r\n    box-sizing: border-box;\r\n    \r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n.navbar_links{\r\n    color: #E9E8E8;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    text-decoration: none;\r\n    transition: all 0.3s ease;\r\n    cursor: pointer;\r\n    position: relative;\r\n    overflow-x: unset;\r\n}\r\n\r\n\r\n\r\n/* hover effect on navbar elements */\r\n.navbar_links:hover{\r\n    color: #913175;\r\n    transition: all 0.3s ease;\r\n}\r\n\r\n\r\n.navbar_links:after{\r\n    content: \"\";\r\n    position: absolute;\r\n    background-color: #913175;\r\n    height: 3px;\r\n    width: 0;\r\n    left: 0;\r\n    bottom: -10px;\r\n    transition: 0.3s;\r\n}\r\n\r\n.navbar_links:hover:after{\r\n    width: 100%;\r\n}\r\n\r\n\r\n@media screen and (max-width: 900px){\r\n\r\n    /* navigation links */\r\n    nav{\r\n        display: flex;\r\n        position: relative;\r\n        padding: 15px 20px;\r\n    }\r\n\r